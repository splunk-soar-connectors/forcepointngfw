            "data_type": "string",
            "order": 0
        },
        "verify_server_cert": {
            "data_type": "boolean",
            "description": "Verify server certificate",
            "order": 1,
            "default": false
        },
        "base_port": {
            "required": true,
            "description": "SMC API Port",
            "data_type": "string",
            "order": 2
        },
        "auth_key": {
            "required": true,
            "description": "SMC API Auth Key",
            "data_type": "password",
            "order": 3
        },
        "base_version": {
            "required": true,
            "description": "SMC Version (e.g. 6.2)",
            "data_type": "string",
            "order": 4
        }
    },
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validate the asset configuration for connectivity using supplied configuration",
            "verbose": "Tests api cpnnectivity by making a login/logout call to SMC api.",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "block ip",
            "identifier": "block_ip",
            "description": "Block an IP",
            "verbose": "Updates Bad IP list.",
            "type": "contain",
            "read_only": false,
            "parameters": {
                "ip": {
                    "description": "IP to block",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "ip"
                    ],
                    "order": 0
                },
                "group": {
                    "description": "Group Name to add the IP",
                    "data_type": "string",
                    "required": true,
                    "order": 1
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "column_name": "Status",
                    "column_order": 1,
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.parameter.group",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "column_name": "IP",
                    "column_order": 0
                },
                {
                    "data_path": "action_result.data",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric"
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric"
                }
            ],
            "render": {
                "type": "table"
            },
            "versions": "EQ(*)"
        }
